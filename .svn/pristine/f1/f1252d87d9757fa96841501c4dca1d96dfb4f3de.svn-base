html, body{width:100%;}

.wrap{width:100%; min-width:1200px;}
.wrap_in{margin:0 auto; width:100%; max-width:1600px;}
.in{margin:0 auto; width:100%; max-width:1200px;}

.wrap .wrap_header{width:100%; height:98px; border-bottom:3px solid #239aeb;}
body.dev .wrap .wrap_header{border-bottom:3px solid #e64655;}
.wrap .wrap_header .wrap_in{height:100%;}
.wrap .wrap_header .in{height:100%;}
.wrap .wrap_header .logo{display:inline-block; position:relative; top:0; left:0; height:98px; padding:31px 16px 0 21px;}
.wrap .wrap_header .logo:before{position:absolute; bottom:0; left:0; width:100%; height:3px; background:#1c435f; content:'';}

.notice_wrap.show{opacity:1; z-index:999;}
.notice_wrap{position:fixed; left:0; right:0; top:0; bottom:0; background:rgba(21,21,21, .5); transition:opacity 300ms; opacity:0; z-index:-1;}
.notice_wrap .notice_list{position:fixed; top:50%; left:50%; transform:translate(-50%,-45%); width:100%; max-width:1200px; height:82%; max-height:830px;  background:#fff;}
.notice_wrap .notice_list .notice_list_in{margin-top:-76px; width:100%; height:100%;}
.notice_iframe{width:100%; height:100%; background:#fff;
	-webkit-transform: translate3d(0,0,0);
	backface-visibility: hidden;
}
/*.notice_wrap{width:100%; height:100%; overflow-y:auto;}*/
.notice_wrap .title_set{position:relative; padding:26px 0 0 54px; width:100%; height:76px; background:#239aeb;}
.notice_wrap .title_set .title{font-size:28px; color:#fff; font-weight:400;}
.notice_wrap .title_set button{display:inline-block; position:absolute; top:25px; right:37px; background:transparent; outline:0; border:0;}

/* Iframe */
/*공지사항리스트*/
.notice_list_set .inner_set{padding:59px 97px 30px; width:100%;}
.notice_list_set .inner_set .notice_table{text-align:center;}
.notice_list_set .inner_set .notice_table thead{}
.notice_list_set .inner_set .notice_table thead tr{border-top:1px solid #1c435f;}
.notice_list_set .inner_set .notice_table thead tr th{padding:18px 0 15px; font-size:14px; font-weight:600; color:#000; border-bottom:1px solid #d9dfe4;}
.notice_list_set .inner_set .notice_table thead tr th.w01{width:63px;}
.notice_list_set .inner_set .notice_table thead tr th.w02{width:746px;}
.notice_list_set .inner_set .notice_table thead tr th.w03{width:115px;}
.notice_list_set .inner_set .notice_table thead tr th.w04{width:107px;}
.notice_list_set .inner_set .notice_table tbody{}
.notice_list_set .inner_set .notice_table tbody tr{}
.notice_list_set .inner_set .notice_table tbody tr td{padding:18px 0 15px; border-bottom:1px solid #d9dfe4; font-size:14px; color:#000;}
.notice_list_set .inner_set .notice_table tbody tr td.bold a{display:inline-block; margin-right:14px; font-size:16px; font-weight:400; vertical-align: baseline;}
.notice_list_set .inner_set .notice_table tbody tr td.bold a:hover{text-decoration:underline;}
.notice_list_set .inner_set .notice_table tbody tr td.bold .ico{display:inline-block; width:15px; height:14px; vertical-align: text-top; background:url("../images/ico_material.png") no-repeat 0 0;}
.notice_list_set .inner_set .notice_table tbody tr td.bold{padding-left:43px; text-align:left;}
.notice_list_set .paging_set{padding-bottom:85px; text-align:center; font-size:0; vertical-align: middle;}

.notice_list_set .paging_set a {display:inline-block; margin-left:-1px; width:35px; height:35px; font-size:12px; color:#000; line-height:35px; border:1px solid #d9dfe4; vertical-align:baseline; background:#fff;}
.notice_list_set .paging_set .arrow{display:inline-block; margin-left: 0; padding:0 10px; background:transparent; border:1px solid transparent; vertical-align:baseline;}
.notice_list_set .paging_set .arrow img {width: auto;max-width: 100%;}
.notice_list_set .paging_set .number.on{font-weight:600;}

/*공지사항내용*/
.notice_view_set{width:100%;}
.notice_view_set .inner_set{padding:33px 97px; width:100%; max-width:1200px;}
.notice_view_set .inner_set .view_table{width:100%; text-align:left;}
.notice_view_set .inner_set .view_table:first-child{width:100%; border-top:1px solid #1c435f;}
