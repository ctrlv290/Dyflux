.notice_view_set .inner_set .view_table tr td{font-size:16px; color:#000; font-weight:600;}
.notice_view_set .inner_set .view_table tr th.lookup{width:115px;}
.notice_view_set .inner_set .view_table tr td.lookup{width:76px;}
.notice_view_set .inner_set .view_table tr .thin{font-weight:400;}
.notice_view_set .inner_set .view_inner{ }
.notice_view_set .inner_set .view_inner td{padding:37px 28px; font-size:16px; color:#000; line-height:24px; font-weight:400 !important;}
.notice_view_set .inner_set .btn_set{margin-top:20px; width:100%; font-size:0;}
.notice_view_set .inner_set .btn_set a{padding:10px 27px; font-size:14px; color:#1c435f; font-weight:400; outline:0; background:#fff; border:1px solid #aab4c2; border-radius:3px;}
.notice_view_set .inner_set .btn_set .prev{margin-right:5px;}
.notice_view_set .inner_set .btn_set:after{display:block; content:''; clear:both;}
.notice_view_set .inner_set .btn_set .list{float:right; color:#fff; border:1px solid #1c435f; background:#1c435f;}






