.wrap_page_in .form_sale_set input{display:inline-block; padding:0 4px; /*width:56%;*/ height:24px; border: 1px solid #1c435f; vertical-align: middle; box-sizing:border-box;}

.wrap_page_in .page_line .select_set .txt {color: #000;font-size: 13px;line-height: 1;vertical-align: baseline;font-weight: 400;}

.wrap_main .page_tabs{font-size:0; text-align:left;}
.wrap_main .page_tabs.pdlr20{ padding:0 20px; }
.wrap_main .page_tabs a{display:inline-block; width:80px; padding:10px; font-size:12px; color:#000; text-align:center; border:1px solid #1c435f; border-right:none;}
.wrap_main .page_tabs a.on{color:#fff; background:#1c435f;}
.wrap_main .page_tabs a:last-child{border-right:1px solid #1c435f;}
.wrap_main .wrap_inner .page_tabs{padding:18px 0;}


.wrap_main .table_style05{/*margin:10px 0 30px;*/ width: 100%; border: 1px solid #1c435f; box-shadow: none; table-layout: fixed;}
.wrap_main .table_style05 thead{background: #1c435f;}
.wrap_main .table_style05 thead th{ padding: 10px 0; font-size:12px; color: #fff; letter-spacing: -0.2px;}
/*.wrap_main .table_style05 thead th:not(:last-child){ border-right: 1px solid #142d3f; }*/
.wrap_main .table_style05 thead th{ border-right: 1px solid #142d3f; }
.wrap_main .table_style05 tbody th{padding:6px 5px; font-size:12px; color:#142d3f; background:#b9cedc; border-right: 1px solid #142d3f; border-bottom: 1px solid #142d3f;border-collapse: collapse;}
.wrap_main .table_style05 tbody td{font-size:12px; text-align:center; height: 30px; border-right: 1px solid #142d3f; border-bottom: 1px solid #142d3f;border-collapse: collapse;}
.wrap_main .table_style05 tbody td .gray{color:inherit; opacity:.6;}


.wrap_scroll .table_style05{margin: 0;box-shadow: none;}

.dimmer_set {position: fixed;top: 0;left: 0;bottom: 0;right: 0;z-index: -999;opacity: 0;background-color: rgba(0, 0, 0, .4);
	-webkit-transition-property: all;
	-ms-transition-property: all;
	-o-transition-property: all;
	transition-property: all;
	-webkit-transition-duration: 0.25s;
	-ms-transition-duration: 0.25s;
	-o-transition-duration: 0.25s;
	transition-duration: 0.25s;
	-webkit-transition-timing-function: ease-out;
	-ms-transition-timing-function: ease-out;
	-o-transition-timing-function: ease-out;
	transition-timing-function: ease-out;
}
.dimmer_set.show {z-index: 999;opacity: 1;}