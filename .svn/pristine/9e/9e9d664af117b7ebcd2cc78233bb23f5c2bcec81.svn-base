.wrap_login{display:table-cell; width:100%; height:100%; text-align:center; background:linear-gradient(345deg, #1e8bd5 50%, #239aeb 50%);  background-size:cover;}
.wrap_login .logo{display:inline-block;  padding-bottom:42px; }
.wrap_login .logo img{ width:115px;}
.wrap_login .login_form{padding:0 15.33vw;}
.wrap_login .login_form .input_set{width:100%;}
.wrap_login .login_form .input_set label{display:block; width:100%;}
.wrap_login .login_form .input_set label:not(:last-child){margin-bottom:8px;}
.wrap_login .login_form .input_set label input{padding:0 20px; width:100%; height:47px;  font-size:15px; color:#666; font-family: 'Nanum Barun Gothic', sans-serif; border:0; outline:0; box-sizing:border-box; border-radius: inherit;}
.wrap_login .login_form .login_chk_set{ text-align:left; }
.wrap_login .login_form .login_chk_set label{display:inline-block;  margin-top:12px; margin-bottom:27px;  height:14px;  font-size:12px; color:#fff;  vertical-align: baseline;}
.wrap_login .login_form .login_chk_set .chk_btn[type="checkbox"]{position:relative; margin:0 9px 0 0; display:inline-block;  width:14px; height:14px; border:1.5px solid #fff; background:transparent; outline:0; border-radius:1.5px; vertical-align: bottom;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
}
.wrap_login .login_form .login_chk_set .chk_btn[type="checkbox"]:checked:before{content:''; position:absolute; top:-0.3vw; left:-0.3vw; width:12px; height:12px; background:#fff; border-radius:0.4vw; vertical-align: baseline;}
.wrap_login .login_form .login_chk_set .name{display:inline-block;}
.wrap_login .login_form .login_btn{display:inline-block;  padding:17px 0; width:100%; font-size:15px; color:#fff; background:#1c435f;}

.wrap_header{position:relative; width:100%;  height:50px; text-align:center; background:#239aeb;}
.wrap_header .menu{display:inline-block; padding:0; position:absolute; top:50%; left:20px; width:22px; outline:0; border:0; background:transparent; transform: translateY(-50%);}
.wrap_header .logo{display:inline-block; font-size:17px; color:#fff; font-weight:600; line-height:52px;}
.wrap_header .logo img{ width:73.5px;}

.wrap_bg{display:none; position:fixed; top:0; left:0; right:0; width:100%; height:100%; background-color:rgba(0,0,0,.6); z-index:90;}
.wrap_menu{position:fixed; top:0; left:-150%; width:89.33%; max-width:330px; min-width:320px; height:100%; box-shadow:0 8px 8px 4px rgba(0, 0, 0, 0.8); z-index:100;}
.wrap_menu .navigation{width:100%; height:100%; background:#fff; overflow-y:auto;}
.wrap_menu .navigation .top_logo{padding:18px 18px 0 25px; width:100%;  height:50px; vertical-align: middle; background:#1c435f;}
.wrap_menu .navigation .top_logo p{display:inline-block;  font-size:16px; color:#fff; font-weight:300; vertical-align: baseline;}
.wrap_menu .navigation .top_logo p .name{font-size:inherit; color:#fff; vertical-align: baseline;}
.wrap_menu .navigation .top_logo:after{display:block; content:''; clear:both;}
.wrap_menu .navigation .top_logo button{display:inline-block; padding:0; float:right; outline:0; border:0; background:none;}
.wrap_menu .navigation .top_logo button img{ width:19.5px; max-width:39px;}
.wrap_menu .navigation .gnb_set{width:100%;}
.wrap_menu .navigation .gnb{ padding:0 0 0 20px; width:100%; background:#239aeb; border-bottom:10px solid #f0f1f4;}
.wrap_menu .navigation .gnb > li:not(:last-child){border-bottom:1px solid #1275b8;}
.wrap_menu .navigation .gnb > li > a{display:block;  padding:16px 0 16px 7.5px;  font-size:14px; color:#fff; background:url("../images/menu_off.png") no-repeat right 22.5px center; background-size:11px;
	-webkit-transition: all .1s ease-in;
	-moz-transition: all .1s ease-in;
	transition: all .1s ease-in;
}
.wrap_menu .navigation .gnb > li > a.on{font-weight:600; background:url("../images/menu_on.png") no-repeat right 22.5px center; background-size:11px;
	-webkit-transition: all .1s ease-in;
	-moz-transition: all .1s ease-in;
	transition: all .1s ease-in;
}

.wrap_menu .navigation .gnb > li > a.home {background: none;}

.wrap_menu .navigation .depth{display:none; background:#1e8bd5; border-top:1px solid #1275b8;}
.wrap_menu .navigation .depth li{}
.wrap_menu .navigation .depth li a{display:block; position:relative; padding:17px 0 17px 23px; font-size:14px; color:#fff; font-weight:300;}
.wrap_menu .navigation .depth li a:before{content:''; position:absolute; top:22px; left:12px; width:3px; height:3px; background:#fff; border-radius:50%;}
.wrap_menu .btn_set{width:100%;  height:45px; font-size:0; text-align: center; border-top:1px solid #bac6cf; border-bottom:1px solid #bac6cf;}
.wrap_menu .btn_set:before{display:block; content:''; clear:both;}
.wrap_menu a.btn{display:block; float:left; padding:15px 0; width:50%; font-size:14px; color:#000;}
.wrap_menu a.btn:first-child{border-right:1px solid #bac6cf;}

.wrap_page{width:100%; border-bottom:10px solid #f0f1f4;}
.wrap_page .wrap_page_in{ padding:18px 0; width:100%; text-align:center; border-bottom:1px solid #bac6cf; vertical-align:middle;}
.wrap_page a{display:inline-block; padding:0 44px; vertical-align:baseline;}
.wrap_page a img{width:9px; max-width:19px;}
.wrap_page span{display:inline-block;  font-size:16px; color:#1c435f; font-weight:600; line-height:12px;  vertical-align:bottom;}
.wrap_page span.year{position:relative; margin-right:9px; padding-right:4px;}
.wrap_page span.year:before{content:''; position:absolute; bottom:2px; right:0;  width:2.5px; height:2.5px; background:#1c435f; border-radius:50%;}

.wrap_inner{padding:0 20px;}

.wrap_main{  width:100%; overflow-y:auto; }
.wrap_main.main_inner{margin:34px 0;}
.wrap_main .table_set{ margin-bottom:19px; width:100%;}

.wrap_main dl{padding:14px 0; width:100%;}
.wrap_main dl dt{display:inline-block;  padding-left:16px; margin-left:2px; margin-right:6px;  font-size:14px; color:#000; font-weight:600; vertical-align: middle; line-height:24px; background:url("../images/ico-title.png") no-repeat 0 center; background-size:11px;}
.wrap_main dl dd{display:inline-block;  font-size:14px; color:#000; vertical-align: middle;  line-height:15px; }

.wrap_main .table_style01{ width:100%; text-align:center; border:1px solid #1c435f; box-shadow: 5px 8px 8px 0px rgba(186, 198, 207, 0.24); }
.wrap_main .table_style01 tr:not(:last-child){border-bottom:1px solid #bac6cf;}
.wrap_main .table_style01 tr th{padding:0 0 0 13.5px;  font-size:13px; color:#000; background:#f0f1f4;}
.wrap_main .table_style01 tr td{ padding:15px 0 13.5px 13px;  font-size:13px; color:#000; background:#fff;}

.form_top{  padding:19px 0 0 0; width:100%; overflow-y:auto; }
.form_top .sale{ padding:0 24px;  width:136px;  height:31px; font-size:14px; color:#000; border:1px solid #1c435f; border-radius:1.5px; outline:0;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	box-sizing:border-box;
	background:url("../images/select_arrow.png") no-repeat right 20px center;

	background-size:12px;
}
.form_top{margin:0 0 11px 0; }
.form_top:before{content:''; clear:both; display:block;}
.form_top .cash_set{display:inline-block;  margin-top:8px; float:right; vertical-align:middle;}
.form_top .cash_set .cash{display:inline-block; font-size:16px; color:#1c435f; font-weight:600; vertical-align:baseline;}
.form_top .cash_set .txt{display:inline-block;  font-size:16px; color:#1c435f; vertical-align:baseline;}

.wrap_main table{width:100%; border:1px solid #1c435f; box-shadow: 5px 8px 8px 0px rgba(186, 198, 207, 0.24);table-layout: fixed;}

.wrap_main .table_style02{/}
.wrap_main .table_style02 tr{width:100%; height:42px; text-align:center;}
.wrap_main .table_style02 tr:not(:last-child){border-bottom:1px solid #bac6cf;}
.wrap_main .table_style02 tr th{background:#f0f1f4; font-size:13px; color:#000; letter-spacing:0.1px;}
.wrap_main .table_style02 tr th span{display:inline-block; margin-left:17px; font-size:inherit; color:inherit; vertical-align:text-top;}
.wrap_main .table_style02 tr td{ padding:14px 0; font-size:13px; color:#000;}
.wrap_main .table_style02 tr td:last-child{padding-left:9px;}

.wrap_main .bar{position:relative; width:99.9%; height:20px; background:#bac6cf;}
.wrap_main .bar:before{content:''; position:absolute; top:50%; left:50%; width:45px; height:2px; background:#fff; transform:translate(-50%,-50%);}

.wrap_main .table_style03{}
.wrap_main .table_style03 thead{background:#1c435f;}
.wrap_main .table_style03 thead th{ padding:10px 0; font-size:11px; color:#fff; letter-spacing:-0.2px;}
.wrap_main .table_style03 thead th:not(:last-child){border-right:1px solid #bac6cf;}
.wrap_main .table_style03 tbody td:not(:last-child){border-right:1px solid #bac6cf;}
.wrap_main .table_style03 tbody{}
.wrap_main .table_style03 tbody td{ padding:14px 9px;  height:40px;  font-size:11px; color:#000;}
.wrap_main .table_style03 tbody tr:not(:last-child){border-bottom:1px solid #bac6cf;}
.wrap_main .table_style03 tbody th{background:#1c435f;padding:14px 0; font-size:11px; color:#fff; letter-spacing:-0.2px;}

.wrap_main .table_style03.lowHeight tbody th{padding:6px 0;}
.wrap_main .table_style03.lowHeight tbody td{padding:6px 9px; height: 30px;}

.wrap_main table tbody tr th.elipsis, .wrap_main table tbody tr td.elipsis {white-space: nowrap;text-overflow: ellipsis; overflow: hidden;}


.pagination{margin:20px 0; text-align:center;}
.pagination li{display:inline-block; padding:0; margin:0; font-size:0; line-height:0;}
