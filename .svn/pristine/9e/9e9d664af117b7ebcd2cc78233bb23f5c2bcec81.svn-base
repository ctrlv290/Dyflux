.pagination li a.on{color:#fff; background:#1c435f;}
.pagination li a.pager{padding:7px 6px; border:0;}

.wrap_scroll{ width:100%; overflow-x:auto; /*box-shadow:5px 8px 8px 0px rgba(186, 198, 207, 0.24);*/}
