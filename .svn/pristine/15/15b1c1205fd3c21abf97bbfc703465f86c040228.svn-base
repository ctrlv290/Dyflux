/* btn */
.btn {display:inline-block; height:22px; padding:0 10px; border:1px solid #9fa8b0; border-radius:2px; line-height:20px;background-color: #fff;}
.btn:hover {background-color:#f0f1f4; color:#000;}
.btn i {margin-right:2px; vertical-align:baseline;}
.btn:hover i {color:#fff;}
.a:hover i {color:#fff;}
.xsmall_btn {display:inline-block; height:18px; padding:0 10px; border:1px solid #333; border-radius:2px; font-size:11px; line-height:16px;background-color: #fff;}
.xsmall_btn:hover {background-color:#f0f1f4; color:#000;}
.xsmall_btn i {margin-right:2px; font-size:10px; vertical-align:baseline;}
.xsmall_btn:hover i {color:#fff;}
.small_btn {display:inline-block; height:20px; padding:0 10px; border:1px solid #333; border-radius:2px; font-size:11px; line-height:18px;background-color: #fff;}
.small_btn:hover {background-color:#f0f1f4; color:#000;}
.small_btn i {margin-right:2px; vertical-align:baseline;}
.small_btn:hover i {color:#fff;}
.large_btn {display:inline-block; height:30px; padding:0 10px; border:1px solid #333; border-radius:2px; line-height:28px;background-color: #fff;}
.large_btn i {margin-right:2px; vertical-align:baseline;}
.large_btn:hover, .large_btn.on {background-color:#f0f1f4; color:#000;}
/*.large_btn:hover i, .large_btn.on i {color:#fff;}*/
.large_btn:hover i, .large_btn.on i {color:#000;}

.big_btn {display:inline-block; height:40px; padding:0 20px; border:1px solid #1c435f; border-radius:2px; line-height:38px;background-color: #fff;}
.big_btn i {margin-right:2px; vertical-align:baseline;}
.big_btn:hover {background-color:#1c435f; color:#fff;}
.big_btn:hover i {color:#fff;}

.wide_btn {display:inline-block; height:20px; padding:0 20px; border:1px solid #1c435f; border-radius:2px; line-height:18px;background-color: #fff;vertical-align: middle;}
.wide_btn i {margin-right:2px; vertical-align:baseline;}
.wide_btn:hover {background-color:#1c435f; color:#fff;}
.wide_btn:hover i {color:#fff;}

.icon-size-m i.fas {font-size: 12px;}


.gray_btn {border:1px solid #ccc; background-color:#fff; color:#333;}
.gray_btn:hover {border-color:#adadad; background-color:#e6e6e6; color:#333;}
.gray_btn i {color:#333; margin-right:2px;}
.orange_btn {border:1px solid #fa4c06; background-color:#fff; color:#fa4c06;}
.orange_btn:hover {border-color:#e98a64; background-color:#f67a48; color:#fff;}
.orange_btn i {color:#fa4c06; margin-right:2px;}
.green_btn {color:#7ac063; background-color:#fff; border-color:#7ac063;}
.green_btn:hover {color:#fff; background-color:#7ac063;}
.green_btn i {color:#7ac063;}
.large_btn.blue_btn {color:#223390; border-color:#223390; background-color:#fff;}
.large_btn.blue_btn:hover {color:#fff; background-color:#223390;}
.large_btn.blue_btn i {color:#223390;}
.blue_btn {color:#223390; border-color:#223390; background-color:#fff;}
.blue_btn:hover {color:#fff; background-color:#223390;}
.blue_btn i {color:#223390;}
