.btn_set {margin:10px 0;position: relative;}
.btn_set label input {vertical-align:middle;}
.btn_set .left {position: absolute;top: 0;left: 0;}
.btn_set .right {position: absolute;top: 0;right: 0;}
.btn_set .right  .label_set {display:inline-block; padding:6px 0;}
.btn_set .center {text-align: center;}
.btn_set.max1200 {max-width: 1200px;}
.grid_set {vertical-align: top;}
.grid_box {display: inline-block;vertical-align: top;}
.grid_box_gap {width: 50px;text-align: center;}
.grid_box_gap i {font-size: 40px;margin-top: 200px; color: #d2d9df;}
.grid_box_40 {width: 40%;}
.grid_box_50 {width: calc(60% - 60px);}
.grid_btn_set_top {
	background-color: #d2d9df;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	padding-top: 12px;
	padding-bottom: 12px;
	padding-left: 16px;
	padding-right: 16px;
	line-height: 20px;
}
.grid_btn_set_top .right {float:right;}
.grid_btn_set_top .right  .label_set {display:inline-block; padding:6px 0;}

.grid_btn_set_top input {vertical-align: baseline;}
.grid_btn_set_top input[type="checkbox"] {vertical-align: middle;}
.grid_btn_set_top select {vertical-align: baseline;}
.grid_btn_set_top a {vertical-align: baseline;}
.grid_btn_set_top span {vertical-align: baseline;}
.grid_btn_set_top label {vertical-align: baseline;cursor: pointer;}

.grid_btn_set_top h3 {display:block;margin-bottom: 10px;font-size: 16px;line-height: 20px;font-weight: bold;color: #000;}

