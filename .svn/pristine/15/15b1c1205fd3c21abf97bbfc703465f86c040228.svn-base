/* common */
input[type='text'], input[type='password'] {border:1px solid #ddd; height:20px; line-height:18px; margin:0; vertical-align:middle;padding-left: 3px;}
input[type='checkbox'] {vertical-align:middle;}
textarea {display:block; border:1px solid #ddd; }
select {height:22px; line-height:18px; border:1px solid #ddd; vertical-align:middle;}
label {height:22px; vertical-align:middle;}
p {font-size:12px; line-height:18px; margin:10px 0;}
dl dt {margin:15px 0 10px; font-size:20px; font-weight:700;}
dl dd {margin:0 0 10px; font-size:12px; line-height:18px;}
.ulList, .olList {margin:10px 0;  padding-left:15px;}
.olList li {list-style:decimal; line-height:18px;}
.ulList li {list-style:disc; line-height:18px;}
.bold {font-weight:700 !important;}
.color {color:red;}
p > span {vertical-align:baseline}
.dis_none {display: none !important;}
.dis_inline {display:inline-block; margin:10px 0; vertical-align:middle;}
strong {font-weight: bold;}
div.row {margin-top: 5px;}
.input_file_hidden {position: absolute;top: 0;left: 0;width: 100%;height: 100%;opacity: 0;cursor: pointer;}
.btn_relative {position: relative;cursor: pointer;}
.uploaded-file {vertical-align: baseline;}

.input_label {display: block;padding-right: 10px;white-space: nowrap;cursor: pointer !important;}
.input_label input {vertical-align: middle;}
.input_label span{vertical-align: middle;}

.label_pointer {cursor: pointer;}

.div_h400_scroll {display: block;width: 100%;height: 400px;overflow-x: hidden;overflow-y: scroll;}

html.no-scroll {overflow: hidden;}
.div_center {text-align: center;}
.div_right {text-align: right;}

.f-right {float: right;}

span.strong {color: #000;font-weight: 700;font-size: 12px;font-weight: 700;line-height: 18px;}

/* font_small */
table.font_small thead th,
table.font_small thead td,
table.font_small tbody th,
table.font_small tbody td,
table.font_small tfoot th {font-size:11px; line-height:18px; padding:2px 10px; font-weight: normal;}
table.font_small input[type='text'] {font-size:11px; height:16px; line-height:16px;}
table.font_small input[type='checkbox'] {margin:0;}
table.font_small select {font-size:11px; height:18px; line-height:16px;}
table.font_small tfoot td {font-size:11px; line-height:18px;}
table.font_small tbody td span {font-size:11px; line-height:18px;}
table.font_small div,
table.font_small tbody td .info_txt,
table.font_small tbody td p,
table.font_small tbody td textarea,
table.font_small tbody td label,
table.font_small tfoot td .text {font-size:11px; line-height:18px;}
table.font_small a {line-height:16px;}

/* table */
table {border-top:1px solid #d2d9df; border-left:1px solid #d2d9df;}
table thead th,
table thead td,
table tbody th,
table tbody td,
table tfoot th,
table tfoot td {padding:5px 10px; font-size:12px; line-height:18px;}
table tbody td p {margin:0; font-size:12px; line-height:18px;}
table thead th,
table tbody th,
table tbody td,
table thead td {text-align:center;}
table thead th {color:#333;}
table.color_table thead th,
table.color_table tbody th,
table.color_table tfoot td,
table.color_table tfoot td .text {color:#000; vertical-align:baseline}
table thead th,
table tbody th,
table tfoot th {font-weight:700; border-right:1px solid #ddd; background-color:#f0f1f4;}
table tfoot a {vertical-align:middle;}
table thead td,
table tbody td,
table tfoot td {border-right:1px solid #d2d9df;}
table thead th,
table tbody th,
table tbody td,
table tfoot td {border-bottom:1px solid #d2d9df;word-break: keep-all;}
table td.text_left, table th.text_left {text-align:left;}
table td.text_right, table th.text_right {text-align:right;}
table td.text_center, table th.text_center {text-align:center !important;}
table td .td_row {margin-bottom: 5px;}
table td .td_row:last-child {margin-bottom: 0;}
table td.vtop, table th.vtop {vertical-align: top;}
table td.ellipsis {text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}

table td a.link {font-weight: bold;color: #1c435f;}
table td a.link:hover {color: #ff0202;}

table tbody tr.active_tr {background-color: #ffff99;}
table tbody tr td.blank_td {border-right: 0px;}
table tbody tr td.permission_checked {background-color: #FFFCCC;}
table tbody tr.light_gray {background-color: #f3f3f3 !important;}

	/* index */
.index_page .container {padding-top:15px;}
.index_set {margin-bottom:50px;}
.index_set .index_tit {padding-bottom: 5px;margin-bottom:10px; font-size:20px; line-height:32px; font-weight:700;border-bottom: 2px solid #dadada;}
.index_set ul {}
.index_set ul li {line-height:18px;}
.index_set ul li a {font-size:15px; color:blue;}


ul.list_33 {}
ul.list_33 li {display: inline-block;width: 33.3%;font-size: 12px;line-height: 18px;padding: 5px;border-right: 1px solid #d2d9df;border-bottom: 1px solid #d2d9df;}
ul.list_33 li:nth-child(3n) {border-right: none;}
ul.list_33 li:nth-last-child(3) {border-bottom: none;}
ul.list_33 li:nth-last-child(2) {border-bottom: none;}
ul.list_33 li:nth-last-child(1) {border-bottom: none;}

