/*Step Arrow*/
.clearfix:after {clear: both;content: "";display: block;height: 0;}
.content .step_wrap {margin-top: 10px;padding-bottom: 10px;position: relative;}
.content .step_wrap .step_action {}
.content .step_wrap .arrow-steps {float: right;}
.content .step_wrap .arrow-steps .step{font-size:14px;text-align:center;color:#fff;cursor:default;margin:0 3px;padding:10px 10px 10px 30px;min-width:140px;float:left;position:relative;background-color:#2E7EB6;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;transition:background-color 0.2s ease;}
.content .step_wrap .arrow-steps .step:after,.arrow-steps .step:before{content:" ";position:absolute;top:0;right:-17px;width:0;height:0;border-top:19px solid transparent;border-bottom:18px solid transparent;border-left:17px solid #2E7EB6;z-index:2;transition:border-color 0.2s ease;}
.content .step_wrap .arrow-steps .step:before{right:auto;left:0;border-left:17px solid #fff;z-index:0;}
.content .step_wrap .arrow-steps .step:first-child:before{border:none;}
.content .step_wrap .arrow-steps .step:first-child{border-top-left-radius:4px;border-bottom-left-radius:4px;padding:10px 10px 10px 10px}
.content .step_wrap .arrow-steps .step:last-child {border-top-right-radius:4px;border-bottom-right-radius:4px;}
.content .step_wrap .arrow-steps .step:last-child:after {display: none;}
.content .step_wrap .arrow-steps .step span{position:relative;color: #fff;font-size: 16px;line-height: 16px;font-weight: 700;letter-spacing: 1px;}
/*.content .step_wrap .arrow-steps .step span:before{opacity:0;content:"✔";position:absolute;top:-2px;left:-20px;}*/
.content .step_wrap .arrow-steps .step.done span:before{opacity:1;-webkit-transition:opacity 0.3s ease 0.5s;-moz-transition:opacity 0.3s ease 0.5s;-ms-transition:opacity 0.3s ease 0.5s;transition:opacity 0.3s ease 0.5s;}
.content .step_wrap .arrow-steps .step.current{color:#fff;background-color:#239AEB;
	animation: step_active_ani 2s infinite; /* animation-name followed by duration in seconds*/
	-webkit-animation: step_active_ani 2s infinite; /* Chrome and Safari */
}
.content .step_wrap .arrow-steps .step.current:after{border-left:17px solid #239AEB;
	animation: step_active_border_ani 2s infinite; /* animation-name followed by duration in seconds*/
	-webkit-animation: step_active_border_ani 2s infinite; /* Chrome and Safari */
}

.content .step_wrap .sp_btn_wrap {display: inline-block;position: absolute;top: -55px;left: 100px;vertical-align: baseline;}

@keyframes step_active_ani
{
	0%   {background: #239AEB;}
	50%  {background: #71bef3;}
	100% {background: #239AEB;}
}

@keyframes step_active_border_ani
{
	0%   {border-left-color: #239AEB;}
	50%  {border-left-color: #71bef3;}
	100% {border-left-color: #239AEB;}
}

