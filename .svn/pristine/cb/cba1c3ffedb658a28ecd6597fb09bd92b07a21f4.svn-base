/* console */
.console {position: fixed;bottom: 0;left: 0;width: 100%;height: 30px;overflow: auto;background-color: #000;color: #fff;z-index:999999;}
/* .console .por {overflow: hidden;position: relative;top: 0;left: 0;} */
.console .fix {position: fixed;width: 100%;height: 30px;line-height: 30px;background-image: -webkit-linear-gradient(to bottom, #aaa 0, #333 100%);
	background-image: linear-gradient(to bottom, #aaa 0, #333 100%);}
.console .fix span.close {float: right;padding-right: 10px;color: #fff;text-decoration: none;cursor: pointer;}
.console .div_console {overflow: hidden;margin-top: 35px;padding-left: 10px;}

.console .div_console.hide {display:none;}

.dn {margin-bottom: 0 !important;}
.dn .console {display: none;}

.dev_box_fix_btn {float:left;padding-left: 10px;color: #fff;text-decoration: none;cursor: pointer;}

.console .div_console table {}
.console .div_console table tr {}
.console .div_console table tr th {color: #fff;text-align: left;}
.console .div_console table tr td {color: #fff;text-align: left;}